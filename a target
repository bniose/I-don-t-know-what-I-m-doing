class Target 
{
  int myX = 0;
  int myY = 0;
  
  public Target(int x, int y)
  {
    myX = x;
    myY = y;
    
    fill(#FF0D1D);
    ellipse(x,y,100,100);
    fill(#FFFFFF);
    ellipse(x,y,80,80);
    fill(#FF0D1D);
    ellipse(x,y,60,60);
    fill(#FFFFFF);
    ellipse(x,y,40,40);
    fill(#FF0D1D);
    ellipse(x,y,20,20);
  }
}
